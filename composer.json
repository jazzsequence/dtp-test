{
    "name": "jazzsequence/dtp-test",
    "description": "Test the deploy-to-Pantheon workflow with a build pipeline",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Chris Reynolds",
            "email": "chris@jazzsequence.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
        }
    ],    
    "require": {
        "pantheon-systems/pantheon-mu-plugin": "*",
        "pantheon-systems/pantheon-advanced-page-cache": "*",
        "pantheon-systems/wp-native-php-sessions": "*",
        "wpackagist-theme/twentytwentyfive": "*",
        "wpackagist-plugin/akismet": "*"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
